<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <RelativeLayout
        android:layout_width="146dp"
        android:layout_height="41dp"
        android:layout_marginTop="@dimen/dp_35"
        android:layout_gravity="center"
        android:gravity="center">

        <LinearLayout
            android:id="@+id/content"
            android:layout_width="146dp"
            android:gravity="center"
            android:layout_height="41dp"
            android:background="@drawable/video_dialog_progress_bg"
            android:orientation="horizontal">

            <ImageView
                android:layout_width="18dp"
                android:layout_height="@dimen/sp_16"
                android:src="@drawable/detaic_tv_volume" />
<!--            progress_share-->
            <ProgressBar
                android:id="@+id/volume_progressbar"
                style="@style/video_vertical_progressBar_new"
                android:layout_width="86dp"
                android:layout_height="2dp"
                android:layout_marginLeft="10dp"
                android:progressDrawable="@drawable/rounded_horizontal_progressbar"
                android:orientation="horizontal"
                android:max="100" />

        </LinearLayout>
    </RelativeLayout>


</LinearLayout>
