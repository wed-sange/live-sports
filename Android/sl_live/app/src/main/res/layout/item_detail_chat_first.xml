<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/lltItem"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    android:paddingHorizontal="@dimen/dp_12"
    android:paddingTop="@dimen/dp_12">


        <LinearLayout
            android:id="@+id/lltLevel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/shape_r20_f2f3f7"
            android:backgroundTint="@color/c_3334A853"
            android:gravity="center"
            android:paddingHorizontal="7.5dp"
            android:paddingVertical="@dimen/dp_2">

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tvLevel"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:textStyle="bold"
                android:textColor="@color/white"
                android:textSize="@dimen/sp_12"
                android:text="@string/anchor" />
        </LinearLayout>
        <LinearLayout
            android:id="@+id/agentWeb"
            android:layout_marginStart="@dimen/dp_5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" />
     <!--   <WebView
            android:id="@+id/agentWeb"
            android:layout_marginStart="@dimen/dp_5"
            android:layout_marginTop="-5dp"
            android:visibility="gone"
            android:background="@color/trans"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" />-->

</LinearLayout>